<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="jpa03" transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		
		<class>com.ff.model.Item</class>
		<class>com.ff.model.Usuario</class>
		
		<properties>
			<property name="eclipselink.target-server" value="WebLogic"/>
			<property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/calories"/>
			<property name="javax.persistence.jdbc.user" value="CaloriesMySQL"/>
			<property name="javax.persistence.jdbc.password" value="CaloriesMySQL"/>
			
			
			
			
			
			
			<property name="eclipselink.logging.level.sql" value="FINE"/>
			<property name="eclipselink.logging.level" value="INFO"/>
			
			
		</properties>
	</persistence-unit>
</persistence>
